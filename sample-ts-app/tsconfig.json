{
  "compilerOptions": {
      "strict": true,
      "target": "esnext",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "declaration": true,
      "sourceMap": true,
      "emitDeclarationOnly": true,
      "esModuleInterop": true,
      "noUncheckedIndexedAccess": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
  }
}